    "# https://leetcode.com/contest/weekly-contest-248/problems/build-array-from-permutation/\n",
    "def buildArray(nums):\n",
    "    result = []\n",
    "    for i in nums:\n",
    "        result.append(nums[i])\n",
    "    return result\n",
    "print(buildArray(nums = [0,2,1,5,3,4]))\n",
    "print(buildArray([5,0,1,2,3,4]))"
